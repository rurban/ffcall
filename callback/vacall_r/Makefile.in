#	if [ ! -d $(libdir) ] ; then mkdir $(libdir) ; fi
#	$(LIBTOOL_INSTALL) $(INSTALL_DATA) libvacall.la $(libdir)/libvacall.la
	if [ ! -d $(includedir) ] ; then mkdir $(includedir) ; fi
	$(INSTALL_DATA) vacall_r.h $(includedir)/vacall_r.h
